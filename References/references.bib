}
@article{Zhang2007,
abstract = {This paper presents a novel approach for estimating the$\backslash$nparameters for MRF-based stereo algorithms. This$\backslash$napproach is based on a new formulation of stereo as$\backslash$na maximum a posterior (MAP) problem in which both a$\backslash$ndisparity map and MRF parameters are estimated from$\backslash$nthe stereo pair itself. We present an iterative$\backslash$nalgorithm for the MAP estimation that alternates$\backslash$nbetween estimating the parameters while fixing the$\backslash$ndisparity map and estimating the disparity map while$\backslash$nfixing the parameters. The estimated parameters$\backslash$ninclude robust truncation thresholds for both data$\backslash$nand neighborhood terms, as well as a regularization$\backslash$nweight. The regularization weight can be either a$\backslash$nconstant for the whole image or spatially-varying,$\backslash$ndepending on local intensity gradients. In the$\backslash$nlatter case, the weights for intensity gradients are$\backslash$nalso estimated. Our approach works as a wrapper for$\backslash$nexisting stereo algorithms based on graph cuts or$\backslash$nbelief propagation, automatically tuning their$\backslash$nparameters to improve performance without requiring$\backslash$nthe stereo code to be modified. Experiments$\backslash$ndemonstrate that our approach moves a baseline$\backslash$nbelief propagation stereo algorithm up six slots in$\backslash$nthe Middlebury rankings.},
