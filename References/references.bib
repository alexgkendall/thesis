journal = {Bmvc},
keywords = {Adaptive Support Windows,BMVC 2011,PatchMatch,Slanted Surfaces,Stereo Matching,Temporal Stereo,alex,stereo},
mendeley-tags = {alex,stereo},
number = {1},
pages = {14.1--14.11},
title = {{PatchMatch Stereo-Stereo Matching with Slanted Support Windows.}},
url = {http://www.ims.tuwien.ac.at/research/stereo{\_}matching/papers/PatchMatchStereo{\_}BMVC2011.pdf$\backslash$nhttp://www.msr-waypoint.net/pubs/152957/PatchMatchStereo{\_}BMVC2011{\_}6MB.pdf},
